	Definir sueld Como Real;
	
	num1= 0;
	sueld= 0;
	cont=0;
	c= 0;
	d= 0;
	Escribir "¿Cuantos empleados me vas a decir?";
	Leer cont ;
	
	Repetir
		Escribir "Sueldo del empleado " ;  
		cont= cont-1;
		leer sueld; 
